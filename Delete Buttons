-- Eliminar el primer ScreenGui
for _, gui in pairs(game.CoreGui:GetChildren()) do
    if gui:IsA("ScreenGui") and gui.Name == "ScreenGui" then
        gui:Destroy()  -- Elimina el ScreenGui
    end
end

-- Eliminar el segundo ScreenGui
for _, gui in pairs(game.CoreGui:GetChildren()) do
    if gui:IsA("ScreenGui") and gui.Name == "ScreenGui" then
        gui:Destroy()  -- Elimina el ScreenGui
    end
end
